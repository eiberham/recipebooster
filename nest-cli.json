{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/superchef/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "apps/superchef/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps",
  "projects": {
    "rabbitmq": {
      "type": "application",
      "root": "apps/rabbitmq",
      "entryFile": "main",
      "sourceRoot": "apps/rabbitmq/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rabbitmq/tsconfig.app.json"
      }
    },
    "superchef": {
      "type": "application",
      "root": "apps/superchef",
      "entryFile": "main",
      "sourceRoot": "apps/superchef/src",
      "compilerOptions": {
        "tsConfigPath": "apps/superchef/tsconfig.app.json"
      }
    }
  }
}